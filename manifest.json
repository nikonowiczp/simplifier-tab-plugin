{
    "manifest_version": 3,
    "name": "Simplifier Tab Title Changer",
    "version": "1.0",
    "description": "Changes tab titles based on URL patterns.",
    "permissions": ["tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }